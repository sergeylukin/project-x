{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDirs": ["."],
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@astro-nx-depla/nx": ["libs/nx/src/index.ts"],
      "@astro-nx-depla/shared/container": [
        "libs/shared/container/src/index.ts"
      ],
      "@astro-nx-depla/shared/layout": ["libs/shared/layout/src/index.js"],
      "@astro-nx-depla/shared/types/environment": [
        "libs/shared/types/environment/src/index.ts"
      ],
      "@astro-nx-depla/shared/ui": ["libs/shared/ui/src/index.js"],
      "@astro-nx-depla/shared/util/formatting": [
        "libs/shared/util/formatting/src/index.ts"
      ],
      "@astro-nx-depla/shared/util/predict-reading-time": [
        "libs/shared/util/astro-remark-plugin-predict-reading-time/src/index.ts"
      ],
      "@astro-nx-depla/website/app": ["libs/website/app/src/index.ts"],
      "@astro-nx-depla/website/config": ["libs/website/config/src/index.ts"],
      "@astro-nx-depla/website/data-access/url": [
        "libs/website/data-access/url/src/index.ts"
      ],
      "@astro-nx-depla/website/entities/app": [
        "libs/website/entities/app/src/index.ts"
      ],
      "@astro-nx-depla/website/entities/post": [
        "libs/website/entities/post/src/index.ts"
      ],
      "@astro-nx-depla/website/entities/user": [
        "libs/website/entities/user/src/index.ts"
      ],
      "@astro-nx-depla/website/menu-data": [
        "libs/website/menu-data/src/index.ts"
      ],
      "@astro-nx-depla/website/types": ["libs/website/types/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
