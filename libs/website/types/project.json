{
  "name": "website-types",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/website/types/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/website/types",
        "main": "libs/website/types/src/index.ts",
        "tsConfig": "libs/website/types/tsconfig.lib.json",
        "assets": ["libs/website/types/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/website/types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/website/types"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/libs/website/types"
      }
    }
  },
  "tags": []
}
