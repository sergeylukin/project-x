{
  "name": "shared-util-config-provider",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/util/config-provider/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/util/config-provider",
        "main": "libs/shared/util/config-provider/src/index.ts",
        "tsConfig": "libs/shared/util/config-provider/tsconfig.lib.json",
        "assets": ["libs/shared/util/config-provider/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/util/config-provider/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/libs/shared/util/config-provider"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../../coverage/libs/shared/util/config-provider"
      }
    }
  },
  "tags": []
}
